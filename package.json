{
	"name": "codelabhub",
	"displayName": "codelabhub",
	"description": "Use programming practice",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codelabhub.codelabSignUp",
		"onCommand:codelabhub.refreshProblems",

		"onView:codelabhub.fetchAndSaveProblem",
		"onView:codelabhub.deleteProblem",
		"onView:codelabhub.uploadProblem",
		"onView:codelabhub.submitCode",
		"onView:codelabhub.login",
		"onView:codelabhub.logout"

	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codelabhub.refreshProblems",
				"title": "Refresh Problems",
				"icon": {
					"dark": "media/dark/refresh.svg",
					"light": "media/light/refresh.svg"
				},
				"category": "CodeLabHub"
			},
			{
				"command": "codelabhub.login",
				"title": "Login",
				"category": "CodeLabHub"
			},
			{
				"command": "codelabhub.logout",
				"title": "Sign Up",
				"category": "CodeLabHub"
			}
		],

		"viewsContainers": {
			"activitybar": [
				{
					"id": "CodeLabHub",
					"title": "CodeLabHub",
					"icon": "media/dep.png"
				}
			]
		},
		"views": {
			"CodeLabHub": [
				{
					"id": "CodeLabHub-Lab",
					"name": "Lab"
				},
				{
					"id": "CodeLabHub-Login",
					"name": "Login"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "codelabhub.refreshProblems",
					"when": "",
					"group": "navigation"
				},
				{
					"command": "codelabhub.makeLab",
					"when": "",
					"group": "navigation"
				}
			]
		},


		"viewsWelcome": [
			{
				"view": "codelabhub.login",
				"contents": "You are not currently logged in to an account\n[Login](command:extension.getInfo)",
				"when": "!extensionSelectionMode"
			},    
			{
				"view": "codelabhub.logout",
				"contents": "You are currently logged in to an account\n[Logout](command:extension.logout)",
				"when": "extensionSelectionMode"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"ts-loader": "^9.2.8",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	}
}
